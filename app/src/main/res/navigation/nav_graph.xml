<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainMenuFragment">

    <fragment
        android:id="@+id/mainMenuFragment"
        android:name="vyvojmobilnychapk.project3.stgame2.fragments.MainMenuFragment"
        android:label="fragment_main_menu"
        tools:layout="@layout/fragment_main_menu" >
        <action
            android:id="@+id/action_mainMenuFragment_to_settingsFragment"
            app:destination="@id/settingsFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_editGameFragment"
            app:destination="@id/editGameFragment" />
        <action
            android:id="@+id/action_mainMenuFragment_to_gameFragment"
            app:destination="@id/gameFragment" />
    </fragment>
    <fragment
        android:id="@+id/editGameFragment"
        android:name="vyvojmobilnychapk.project3.stgame2.fragments.EditGameFragment"
        android:label="fragment_edit_game"
        tools:layout="@layout/fragment_edit_game" />
    <fragment
        android:id="@+id/gameFragment"
        android:name="vyvojmobilnychapk.project3.stgame2.fragments.GameFragment"
        android:label="fragment_game"
        tools:layout="@layout/fragment_game" />
    <fragment
        android:id="@+id/groupDetailFragment"
        android:name="vyvojmobilnychapk.project3.stgame2.fragments.GroupDetailFragment"
        android:label="fragment_group_detail"
        tools:layout="@layout/fragment_group_detail" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="vyvojmobilnychapk.project3.stgame2.fragments.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
</navigation>